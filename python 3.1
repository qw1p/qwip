def func(nums: list[int], s: int = 2):
    list1 = [num * s for num in nums]
    return list1

l_func = lambda list2, s=2: list(map(lambda x: x * s, list2))

list3 = input("Введите числа через пробел: ").split()
nums = [int(x) for x in list3]
    
s_input = input("Введите множитель (по умолчанию 2): ")
s = int(s_input) if s_input else 2
    
print("Результат (функция):", func(nums, s))
print("Результат (лямбда-функция):", l_func(nums, s))
