import statistics
import random
import math

num = [random.randint(1, 100) for x in range(100)]

print("Медиана ->", statistics.median(num))
print("Среднее значение ->", statistics.mean(num))
print("Стандартное отклонение ->", round(statistics.stdev(num), 2))
print("Округлённое значение корня суммы чисел ->", round(math.sqrt(sum(num)), 2))
